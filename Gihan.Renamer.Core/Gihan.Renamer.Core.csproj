<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Muhammad Babayi</Authors>
    <Company>GihanSoft</Company>
    <Description>base interfaces for Gihan.Renamer</Description>
    <Copyright>Copyright (c) 2018 Mohammad Babayi</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/chiefmb/Renamer/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/chiefmb/Renamer</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/chiefmb/Renamer/master/Renamer/renameIcon.ico</PackageIconUrl>
    <RepositoryUrl>https://github.com/chiefmb/Renamer</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <ApplicationIcon>..\Renamer\renameIcon.ico</ApplicationIcon>
    <PackageId>Gihan.Renamer.Core</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\Renamer\renameIcon.ico" Link="renameIcon.ico" />
  </ItemGroup>

</Project>
